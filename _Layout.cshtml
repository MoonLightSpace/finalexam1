<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"></script>
</head>
<body>
    <nav>
        <div class="nav-wrapper #006064 cyan darken-4" >
            <div class="container">
                <a href="http://localhost:64813/" class="navbar-brand" id="name">Vistas generales visitas</a>
                <ul id="nav-mobile" class="right hide-on-med-and-down">
                    <li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("Visitas", "Index", "Visitas")</li>
                    <li>@Html.ActionLink("Miembros", "Index", "Miembroes")</li>
                </ul>
             </div>
        </div>
    </nav>
    
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer class="page-footer #006064 cyan darken-4">
            <div class="container">
                <div class="row">
                    <div class="col l6 s12">
                        <p class="blue-text text-darken-2"> Leyra Mora Santos   2018/6046</p>
                    </div>
                    
                </div>
            </div>
            <div class="footer-copyright">
                <div class="container">
                    <span class="blue-text text-darken-2">© 2019 Copyright Examen final web</span>
                </div>
                    
                    <a class="grey-text text-lighten-4 right" href="#!">More Links</a>
                </div>
            </div>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
